{% extends 'base.html.twig' %}

{% block title %}Nouveau lieu{% endblock %}
{% block body %}

    {{ form_start(form) }}
    {{ form_errors(form) }}

    <div class="row">
        <div class="col-6">
            {{ form_row(form.address) }}
        </div>
        <div class="col-6">
            {{ form_row(form.type) }}
        </div>
        <div class="col-6">
            {{ form_row(form.iat) }}
        </div>
        <div class="col-6">
            {{ form_row(form.ion) }}
        </div>

        <button type="submit" class="btn btn-success">
            modifier
        </button>
    </div>

    {{ form_end(form) }}

{% endblock %}